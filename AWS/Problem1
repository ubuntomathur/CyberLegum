[root@00-0c-29-fa-75-fb ~]# cat  test.yml
apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app: v1
  name: v1
spec:
  replicas: 3
  selector:
    matchLabels:
      app: v1
  strategy: {}
  template:
    metadata:
      labels:
        app: v1
    spec:
      containers:
      - image: nginx:1.17
        name: nginx




